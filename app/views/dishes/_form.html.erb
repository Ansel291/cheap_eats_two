<%= simple_form_for @dish do |f| %>
  <%= f.input :title, label: "Dish Name:" %>
  <%= f.input :restaurant, label: "Restaurant Name:" %>
  <%= f.input :city, label: "City, State:" %>
  <%= f.input :price, label: "$" %>
  <%= f.input :description, label: "Description:" %>
  <%= f.button :submit  %>
<% end %>
